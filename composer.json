{
    "name": "devineonline/smsbroadcast-laravel-notifcation-channel",
    "description": "SMS Broadcast Notification Channel",
    "homepage": "http://laravel-notification-channels.com/",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "atymic/sms-broadcast": "^1.0",
        "illuminate/notifications": "^8.0",
        "illuminate/support": "^8.0"
    },
    "authors": [
        {
            "name": "devineonline",
            "email": "composer@devineonline.com.au",
            "homepage": "https://devineonline.com.au",
            "role": "Developer"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SmsBroadcast\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\SmsBroadcast\\SmsBroadcastServiceProvider"
            ]
        }
    }
}
